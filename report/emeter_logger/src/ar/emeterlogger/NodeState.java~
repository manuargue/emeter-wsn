package ar.emeterlogger;

public enum NodeState
{
    INACTIVE(false),
    ACTIVE(true);
    
    private boolean s;

    NodeState(boolean s)
    {
        this.s = s;
    }

    public String toString()
    {
        return (this.s ? "Activo" : "Inactivo");
    }
    
    public boolean toBoolean()
    {
        return this.s;
    }    
}

